{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "tsc && nyc mocha",
    "start": "ts-node-dev index.ts",
    "lint": "eslint . --ext .ts"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "pre-push": [
    "test"
  ],
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/sqlite3": "^3.1.7",
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "express-md": "^2.0.2",
    "sqlite3": "^4.0.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "mocha": "^6.1.4",
    "nyc": "^15.1.0",
    "pre-push": "^0.1.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "supertest": "^4.0.2",
    "ts-node": "^10.2.1"
  }
}
